            }
        }
    },
    "pingPort": 3030,
    "datacenter": "coal",
    "domain": "test.coal",
    "objectRoot": "/",
    "server_uuid": "e05a6490-bcad-11e1-afa7-0800200c9a66",
    "zone_uuid": "e91001d0-bcad-11e1-afa7-0800200c9a66",
