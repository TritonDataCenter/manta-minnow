{
    "moray": {
        "bucket": {
            "name": "manta_storage",
            "index": {
                "hostname": { "type": "string" },
                "availableMB": { "type": "number" },
                "percentUsed": { "type": "number" },
                "server_uuid": { "type": "string" },
                "timestamp": { "type": "number" },
                "zone_uuid": { "type": "string" },
                "manta_compute_id": { "type": "string" },
                "manta_storage_id": { "type": "string" }
